{
  "manifest_version": 2,

  "name": "The Void",
  "description": "This extension will make the current page void",
  "version": "1.0",



  "browser_action": {
   "default_icon": "void.png",
   "default_popup": "popup.html"
  },

  "permissions": [
    "webNavigation",
    "*://*/*",
    "activeTab"
   ]
}
