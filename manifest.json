{
  "manifest_version": 2,

  "name": "The Void",
  "description": "This extension will make the current page void",
  "version": "1.0",
  "background": { "scripts": ["void.js"] },
  "permissions": [ "webNavigation", "*://*/*" ],



  "browser_action": {
   "default_icon": "void.png",
   "default_popup": "popup.html"
  },

  "content_scripts": [
   {
     "matches": ["https://play.google.com/*"],
     "css": ["mystyles.css"],
     "js": ["void.js]
   }
 ],


  /*"permissions": [
    "webNavigation",
    "*://*/*",
    "activeTab"
   ]*/
}
